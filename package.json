{
  "name": "@bliplogs/sdk",
  "version": "0.1.2",
  "description": "Zero-dependency event logging SDK for BlipLogs. Track events from browser and server with minimal overhead.",
  "author": "BlipLogs <hello@bliplogs.co.uk>",
  "license": "MIT",
  "homepage": "https://bliplogs.co.uk",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NoSweatWebsites/bliplogs-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/NoSweatWebsites/bliplogs-sdk/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  },
  "keywords": [
    "logging",
    "events",
    "analytics",
    "bliplogs",
    "event-tracking",
    "telemetry",
    "observability",
    "monitoring",
    "sendbeacon",
    "fire-and-forget",
    "zero-dependencies",
    "debugging"
  ],
  "publishConfig": {
    "access": "public"
  }
}
